{
	"name": "@dxdns/feflow-core",
	"version": "0.0.27",
	"description": "",
	"private": false,
	"author": "Di√≥genes Rodrigues <https://github.com/dxdns>",
	"publishConfig": {
		"access": "public"
	},
	"license": "MIT",
	"homepage": "https://feflow.dxdns.dev",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dxdns/feflow.git"
	},
	"scripts": {
		"build": "tsc && node esbuild.config.mjs",
		"dev": "concurrently \"tsc --watch --emitDeclarationOnly --outDir dist\" \"node scripts/banner.mjs && node esbuild.config.mjs --watch\"",
		"postinstall": "node ./postinstall.js"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./utils": {
			"import": "./dist/utils/index.js",
			"types": "./dist/utils/index.d.ts"
		},
		"./types": {
			"import": "./dist/types/index.js",
			"types": "./dist/types/index.d.ts"
		},
		"./actions": {
			"import": "./dist/actions/index.js",
			"types": "./dist/actions/index.d.ts"
		},
		"./icons": {
			"import": "./dist/icons/index.js",
			"types": "./dist/icons/index.d.ts"
		},
		"./styles/*.css": {
			"types": "./dist/styles/*.css.d.ts",
			"default": "./dist/styles/*.css"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"postinstall.js"
	],
	"keywords": [
		"components",
		"ui",
		"ui-kit",
		"react",
		"svelte",
		"next"
	],
	"type": "module",
	"devDependencies": {
		"chalk": "^5.4.1",
		"esbuild": "0.25.9",
		"esbuild-css-modules-plugin": "^3.1.5"
	}
}
