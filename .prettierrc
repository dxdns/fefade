{
	"semi": false,
	"singleQuote": false,
	"vueIndentScriptAndStyle": true,
	"printWidth": 80,
	"tabWidth": 2,
	"bracketSpacing": true,
	"jsxSingleQuote": false,
	"useTabs": true,
	"trailingComma": "none",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-astro"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		},
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
